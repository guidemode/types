{
  "name": "@guideai-dev/types",
  "version": "0.1.35",
  "description": "Shared TypeScript types for GuideAI",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/guideai-dev/types.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./providers": {
      "types": "./dist/esm/providers/index.d.ts",
      "import": "./dist/esm/providers/index.js",
      "require": "./dist/cjs/providers/index.js"
    },
    "./providers/claude": {
      "types": "./dist/esm/providers/claude/index.d.ts",
      "import": "./dist/esm/providers/claude/index.js",
      "require": "./dist/cjs/providers/claude/index.js"
    },
    "./providers/codex": {
      "types": "./dist/esm/providers/codex/index.d.ts",
      "import": "./dist/esm/providers/codex/index.js",
      "require": "./dist/cjs/providers/codex/index.js"
    },
    "./providers/opencode": {
      "types": "./dist/esm/providers/opencode/index.d.ts",
      "import": "./dist/esm/providers/opencode/index.js",
      "require": "./dist/cjs/providers/opencode/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "pnpm run clean && pnpm run build:esm && pnpm run build:cjs && pnpm run build:cjs-package",
    "build:esm": "tsc -p tsconfig.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:cjs-package": "echo '{\"type\":\"commonjs\"}' > dist/cjs/package.json",
    "dev": "tsc --watch",
    "clean": "rm -rf dist *.tsbuildinfo",
    "typecheck": "tsc --noEmit",
    "lint": "biome check . --diagnostic-level=error",
    "lint:all": "biome check .",
    "lint:fix": "biome check . --write"
  },
  "devDependencies": {
    "typescript": "^5.7.2"
  },
  "keywords": ["types", "typescript", "guideai"],
  "dependencies": {
    "zod": "^3.25.0"
  }
}
